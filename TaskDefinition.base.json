{
    "containerDefinitions": [{
		"command": [
            "COMMAND"
		],
		"entryPoint": [
            "sh",
            "-c"
        ],
		"environment": [],
        "essential": true,
        "image": "IMAGE_NAME",
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "fargate-cluster",
                "awslogs-region": "us-east-1",
                "awslogs-stream-prefix": "test-prefix"
            }
        },
        "name": "SERVICE_NAME",
        "portMappings": [{
            "containerPort": 3000,
            "hostPort": 3000,
            "protocol": "tcp"
        }],
        "repositoryCredentials": {
			"credentialsParameter": "DOCKER_HUB_CREDENTIALS"
		}
	}],
	"cpu": "CPU",
    "family": "SERVICE_NAME",
    "executionRoleArn": "ROLE_ARN",
    "memory": "MEMORY",
	"networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
	],
	"taskRoleArn": "ROLE_ARN"
}